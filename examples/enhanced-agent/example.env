# Enhanced Teneo Agent Configuration

# Required: Your Ethereum private key for authentication
PRIVATE_KEY=

# Optional: NFT Configuration (leave NFT_TOKEN_ID empty to auto-mint)
NFT_TOKEN_ID=

# Optional: Agent configuration
AGENT_NAME=Enhanced Example Agent
AGENT_DESCRIPTION=A demonstration agent showcasing the enhanced Teneo Agent SDK capabilities
AGENT_IMAGE=https://example.com/agent-avatar.png
AGENT_VERSION=2.0.0
AGENT_CAPABILITIES=text_processing,data_analysis,conversation,demonstration

# Optional: Health monitoring
HEALTH_ENABLED=true
HEALTH_PORT=8080

# Optional: Room to join
ROOM_ID=general

# Optional: Logging
LOG_LEVEL=info

# Optional: Rate Limiting
RATE_LIMIT_PER_MINUTE=  # Max tasks per minute (0 = unlimited, default)

# Optional: Redis Cache (for persistent storage across restarts)
REDIS_ENABLED=false  # Set to true to enable Redis caching
REDIS_ADDRESS=localhost:6379  # Redis server address (host:port)
REDIS_USERNAME=  # Redis ACL username (Redis 6+, leave empty for legacy auth)
REDIS_PASSWORD=  # Redis password (if required)
REDIS_USE_TLS=false  # Enable TLS/SSL (required for DigitalOcean, AWS, etc.)
# REDIS_DB=0  # Redis database number (0-15, default: 0)
# REDIS_KEY_PREFIX=  # Custom key prefix (auto-generated if empty)